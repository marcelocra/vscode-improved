{
  "name": "One Dark Semantic",
  "type": "dark",
  "colors": {
    // Editor Backgrounds and Text
    "editor.background": "#282c34",
    "editor.foreground": "#abb2bf",
    "editor.selectionBackground": "#3E4451",
    "editor.lineHighlightBackground": "#2C313C",
    "editorCursor.foreground": "#528bff",

    // Sidebar and Panel Backgrounds
    "sideBar.background": "#21252b",
    "sideBar.foreground": "#abb2bf",
    "sideBarSectionHeader.background": "#2C313C",
    "sideBarTitle.foreground": "#abb2bf",

    // Activity Bar
    "activityBar.background": "#21252b",
    "activityBar.foreground": "#abb2bf",
    "activityBarBadge.background": "#e06c75",
    "activityBarBadge.foreground": "#ffffff",

    // Title Bar and Menus
    "titleBar.activeBackground": "#21252b",
    "titleBar.activeForeground": "#9da5b4",
    "titleBar.inactiveBackground": "#21252b",
    "titleBar.inactiveForeground": "#697098",

    // Status Bar
    "statusBar.background": "#21252b",
    "statusBar.foreground": "#9da5b4",
    "statusBar.debuggingBackground": "#e06c75",
    "statusBar.debuggingForeground": "#ffffff",

    // Panels (e.g., Output, Debug Console)
    "panel.background": "#282c34",
    "panel.border": "#21252b",
    "panelTitle.activeForeground": "#abb2bf",
    "panelTitle.inactiveForeground": "#9da5b4",

    // Tabs
    "tab.activeBackground": "#282c34",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#21252b",
    "tab.inactiveForeground": "#697098",
    "tab.border": "#21252b",

    // Editor Gutter
    "editorGutter.background": "#282c34",
    "editorGutter.modifiedBackground": "#61afef",
    "editorGutter.addedBackground": "#98c379",
    "editorGutter.deletedBackground": "#e06c75",

    // Scrollbars
    "scrollbarSlider.background": "#4b5263",
    "scrollbarSlider.activeBackground": "#5a5e7a",
    "scrollbarSlider.hoverBackground": "#5a5e7a",

    // List and Tree (e.g., Explorer)
    "list.activeSelectionBackground": "#2C313C",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#2C313C",
    "list.hoverBackground": "#2C313C",
    "list.dropBackground": "#3E4451",

    // Input and Dropdown
    "input.background": "#3E4451",
    "input.foreground": "#abb2bf",
    "input.border": "#21252b",
    "dropdown.background": "#3E4451",
    "dropdown.foreground": "#abb2bf",

    // Button
    "button.background": "#61afef",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#528bff"
  },
  "semanticTokenColors": {
    "comment": {
      "foreground": "#5c6370",
      "fontStyle": "italic"
    },
    "string": {
      "foreground": "#98c379"
    },
    "number": {
      "foreground": "#d19a66"
    },
    "variable": {
      "foreground": "#e06c75"
    },
    "variable.readonly": {
      "foreground": "#56b6c2"
    },
    "parameter": {
      "foreground": "#d19a66"
    },
    "keyword": {
      "foreground": "#c678dd"
    },
    "operator": {
      "foreground": "#56b6c2"
    },
    "function": {
      "foreground": "#61afef"
    },
    "method": {
      "foreground": "#61afef"
    },
    "property": {
      "foreground": "#e06c75"
    },
    "class": {
      "foreground": "#e5c07b"
    },
    "interface": {
      "foreground": "#e5c07b"
    },
    "namespace": {
      "foreground": "#c678dd"
    },
    "enum": {
      "foreground": "#e5c07b"
    },
    "typeParameter": {
      "foreground": "#e5c07b"
    },
    "type": {
      "foreground": "#c678dd"
    },
    "enumMember": {
      "foreground": "#98c379"
    },
    "event": {
      "foreground": "#56b6c2"
    },
    "modifier": {
      "foreground": "#c678dd"
    },
    "regexp": {
      "foreground": "#56b6c2"
    },
    "struct": {
      "foreground": "#e5c07b"
    },
    "macro": {
      "foreground": "#c678dd"
    },
    "annotation": {
      "foreground": "#98c379"
    },
    "boolean": {
      "foreground": "#d19a66"
    },
    "escapeSequence": {
      "foreground": "#56b6c2"
    },
    "punctuation": {
      "foreground": "#abb2bf"
    },
    "invalid": {
      "foreground": "#ffffff",
      "background": "#e06c75"
    }
  }
}
