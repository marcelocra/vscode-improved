{
  "name": "Post-Apocalyptic Dusty Dawn",
  "type": "light",
  "colors": {
    "editor.background": "#f5f5f2",
    "editor.foreground": "#403d3d",
    "editor.selectionBackground": "#c7c7c2",
    "editor.lineHighlightBackground": "#e0e0dd",
    "editorCursor.foreground": "#b05e0d",

    "sideBar.background": "#e8e8e3",
    "sideBar.foreground": "#494949",
    "panel.background": "#f0f0eb",
    "activityBar.background": "#d9d9d5",
    "activityBar.foreground": "#9c6644",
    "activityBarBadge.background": "#bf5f5f",
    "statusBar.background": "#e1e1dd",
    "statusBar.foreground": "#756e5b",
    "tab.activeBackground": "#e0e0dd",
    "tab.inactiveBackground": "#e8e8e3",

    "editorGutter.background": "#f5f5f2",
    "editorGutter.modifiedBackground": "#8b7d5c",
    "scrollbarSlider.background": "#bababa"
  },
  "semanticTokenColors": {
    "comment": {
      "foreground": "#857f7a",
      "fontStyle": "italic"
    },
    "string": {
      "foreground": "#a07144"
    },
    "number": {
      "foreground": "#5a7b64"
    },
    "variable": {
      "foreground": "#9c6644"
    },
    "variable.readonly": {
      "foreground": "#6b8a91"
    },
    "parameter": {
      "foreground": "#7f6f54"
    },
    "keyword": {
      "foreground": "#a16464"
    },
    "operator": {
      "foreground": "#bfb097"
    },
    "function": {
      "foreground": "#6b8a91"
    },
    "class": {
      "foreground": "#7f6f54"
    },
    "namespace": {
      "foreground": "#5a7b64"
    },
    "enum": {
      "foreground": "#a07144"
    },
    "boolean": {
      "foreground": "#a16464"
    },
    "struct": {
      "foreground": "#6b8a91"
    },
    "macro": {
      "foreground": "#b05e0d"
    },
    "annotation": {
      "foreground": "#5a7b64"
    },
    "type": {
      "foreground": "#7f6f54"
    },
    "property": {
      "foreground": "#a16464"
    },
    // Svelte-specific tokens
    "svelteElement": {
      "foreground": "#9c6644"
    },
    "svelteTag.html": {
      "foreground": "#7f6f54"
    },
    "svelteScript": {
      "foreground": "#5a7b64"
    },
    "svelteStyle": {
      "foreground": "#b05e0d"
    },
    "svelteTransition": {
      "foreground": "#a16464"
    },
    "svelteDirective": {
      "foreground": "#a07144"
    }
  }
}
